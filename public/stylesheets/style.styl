body {
  padding: 30px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  background-color: #3A3A3A
}

body {
  color: #00d6b2
}

a {
  color: #00B7FF
  text-decoration: none;
}

div.test {
  color: #4F4D3F
}

ul {
  list-style-type: none;
  text-indent: 0px;
  list-style-position: none;
  padding-left: 0px;
}

ul.header1 {
  display: inline-block
  float: right
  width: 100%
  text-align: center
}

ul.header1 li {
  display: inline;
  padding-right: 20px;
  font: 20px "Lucida Grande", Helvetica, Arial, sans-serif;
}

.list-left {
  display: block;
  float: left;
  height: 100%;
  min-width: 30%;
  padding-right: 20px;
}

.list-left ul {
  display: inline-block;
  list-style: none;
  padding-left: 0px;
  width: 100%;
}

.list-left ul li {
  style: none;
  width: 100%;
  padding-top: 3px;
  padding-bottom: 3px;
}

.list-left ul li a {
  width: 100%;
  text-decoration: none;
}

.list-left ul li a .list_item {
  min-width: 60px;
  display: inline-block;
  text-decoration: no-underline;
}



.detail {
  display: block;
  float: left;
  width: 50%;
}

.detail .detail_line .label {
  min-width: 200px;
  display: inline-block;
  color: #0b97c4;
}

.detail .detail_line {
  display: block;
  margin-bottom: 5px;
  margin-top: 5px;
}

.detail .detail_line .create_label {
  min-width: 60px;
  display: inline-block;
  color: #0b97c4;
}

.detail .detail_line .create_item_long {
  min-width: 300px;
  display: inline-block;
}

.detail .detail_line .create_item_short {
  min-width: 130px;
  display: inline-block;
}

.post ul {
  display: inline-block
}

.post ul li {
  display: inline;
  padding-right: 20px;
}

.hidden {
  display: none;
}

.chart {
  width: 0px;
  height: 0px;
}

chart-home > svg {
  padding-left: 50px;
  width: 100%;
  height: 450px;
}

